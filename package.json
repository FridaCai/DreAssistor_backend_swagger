{
  "name": "dre-api",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "cnpm install",
    "deploy": "node index.js",

    "exportdb":        "mysqldump -u root -p --databases                   dreassistor                                   > db/dreassistor_db.dump",
    "exportdb-schema": "mysqldump -u root -p --databases --no-data         dreassistor | sed 's/AUTO_INCREMENT=[0-9]*//' > db/dreassistor_schema.dump",
    "exportdb-data":   "mysqldump -u root -p --databases --no-create-info  dreassistor                                   > db/dreassistor_data.dump",

    "importdb_schema": "mysql -u root -p dreassistor < db/dreassistor_schema.dump",
    "importdb_data": "mysql -u root -p dreassistor < db/dreassistor_data.dump",


    "build-prod": "npm install",
    "deploy-prod": "mysqld --console --log_syslog=0",
    "deploy-lenovo": "cnpm install & node index.js",
    "deploy-patac-server": "npm install & node index.js"
  },
  "keywords": [
    "swagger"
  ],
  "license": "Apache-2.0",
  "private": true,
  "dependencies": {
    "connect": "^3.2.0",
    "express": "^4.14.0",
    "fs-extra": "^0.30.0",
    "js-yaml": "^3.3.0",
    "jwt-simple": "^0.5.0",
    "log4js": "^0.6.37",
    "mime": "^1.3.4",
    "moment": "^2.13.0",
    "mysql": "^2.10.2",
    "node-debug": "^0.1.0",
    "swagger-tools": "0.10.1"
  }
}